    "bloomer": "^0.6.5",
    "bulma": "^0.7.4",
    "chart.js": "^2.7.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^1.3.1",
    "jest-dom": "^3.0.2",
    "lint-staged": "^8.1.5",
    "parse-fraction": "1.1.0",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
